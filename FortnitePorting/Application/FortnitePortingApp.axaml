<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:fluentStyle="using:FluentAvalonia.Styling"
             xmlns:ext="clr-namespace:FortnitePorting.Extensions;assembly=FortnitePorting"
             xmlns:ui="clr-namespace:FluentAvalonia.UI.Controls;assembly=FluentAvalonia"
             xmlns:material="clr-namespace:Material.Icons.Avalonia;assembly=Material.Icons.Avalonia"
             xmlns:uip="clr-namespace:FluentAvalonia.UI.Controls.Primitives;assembly=FluentAvalonia"
             xmlns:nodify="https://miroiu.github.io/nodify"
             xmlns:controls="clr-namespace:FortnitePorting.Controls"
             xmlns:breadcrumb="clr-namespace:FluentAvalonia.UI.Controls;assembly=FluentAvalonia.BreadcrumbBar"
             x:Class="FortnitePorting.Application.FortnitePortingApp"
             RequestedThemeVariant="Dark">

     <Application.Resources>
         <ResourceDictionary>
             <ResourceDictionary.MergedDictionaries>
                 <ResourceInclude Source="avares://Nodify/Themes/Generic.xaml" />
                 <ResourceInclude Source="avares://Nodify/Themes/Dark.xaml" />
             </ResourceDictionary.MergedDictionaries>
             
             <ext:EnumToStringConverter x:Key="EnumToString" />
             <ext:EnumToRecordConverter x:Key="EnumToRecord" />
             <ext:EnumHasFlagConverter x:Key="EnumHasFlag" />
             <ext:EnumGreaterOrEqualConverter x:Key="EnumGreaterOrEqual" />
             <ext:EnumEqualsConverter x:Key="EnumEquals" />
             <ext:EnumNotEqualsConverter x:Key="EnumNotEquals" />
             <ext:TitleCaseStringConverter x:Key="TitleCaseString" />
             <ext:UnrealCaseStringConverter x:Key="UnrealCaseString" />
             <ext:StringToDocumentConverter x:Key="StringToDocument" />
         
             <Brush x:Key="ToggleSwitchKnobFillOn">#FFFFFFFF</Brush>
             <Brush x:Key="ToggleSwitchKnobFillOnPointerOver">#FFFFFFFF</Brush>
             <Brush x:Key="ToggleSwitchKnobFillOnPressed">#FFFFFFFF</Brush>
             <Brush x:Key="ToggleSwitchKnobFillOnDisabled">#5DFFFFFF</Brush>
             <Brush x:Key="CheckBoxCheckGlyphForegroundChecked">#FFFFFFFF</Brush>
             <Brush x:Key="CheckBoxCheckGlyphForegroundCheckedPointerOver">#FFFFFFFF</Brush>
             <Brush x:Key="CheckBoxCheckGlyphForegroundCheckedPressed">#FFFFFFFF</Brush>
             <Brush x:Key="CheckBoxCheckGlyphForegroundCheckedDisabled">#5DFFFFFF</Brush>
             <Brush x:Key="ScrollViewerScrollBarsSeparatorBackground">#00000000</Brush>
             <FontFamily x:Key="ContentControlThemeFontFamily">Segoe UI</FontFamily>
             <FontFamily x:Key="SymbolThemeFontFamily">avares://FortnitePorting/Assets/Fonts/Segoe Fluent Icons.ttf#Segoe Fluent Icons</FontFamily>
             <FontFamily x:Key="TimeWasterFontFamily">avares://FortnitePorting/Assets/TimeWaster/Fonts/BurbankBigRegular-Bold.ttf#Burbank Big</FontFamily>
             <Brush x:Key="TabViewItemHeaderBackgroundSelected">#1C000000</Brush>
             <Brush x:Key="TabViewItemHeaderBackgroundPointerOver">#0DFFFFFF</Brush>
             <Brush x:Key="TabViewItemHeaderBackground">#09FFFFFF</Brush>
             <Brush x:Key="TabViewItemSeparator">#00000000</Brush>
             <Brush x:Key="ExpanderHeaderBorderBrush">#78575757</Brush>
             <Brush x:Key="ExpanderHeaderPointerOverBorderBrush">#78575757</Brush>
             <Brush x:Key="ExpanderContentBorderBrush">#78575757</Brush>
             
             <FontFamily x:Key="SFTextLight">avares://FortnitePorting/Assets/Fonts/SF/SF-Pro-Text-Light.otf#SF Pro Text</FontFamily>
             <FontFamily x:Key="SFTextRegular">avares://FortnitePorting/Assets/Fonts/SF/SF-Pro-Text-Regular.otf#SF Pro Text</FontFamily>
             <FontFamily x:Key="SFTextMedium">avares://FortnitePorting/Assets/Fonts/SF/SF-Pro-Text-Medium.otf#SF Pro Text</FontFamily>
             <FontFamily x:Key="SFTextHeavy">avares://FortnitePorting/Assets/Fonts/SF/SF-Pro-Text-Heavy.otf#SF Pro Text</FontFamily>
             <FontFamily x:Key="SFTextSemiBold">avares://FortnitePorting/Assets/Fonts/SF/SF-Pro-Text-Semibold.otf#SF Pro Text</FontFamily>
             <FontFamily x:Key="SFTextBold">avares://FortnitePorting/Assets/Fonts/SF/SF-Pro-Text-Bold.otf#SF Pro Text</FontFamily>
             
             <FontFamily x:Key="SFDisplayThin">avares://FortnitePorting/Assets/Fonts/SF/SF-Pro-Display-Thin.otf#SF Pro Display</FontFamily>
             <FontFamily x:Key="SFDisplayRegular">avares://FortnitePorting/Assets/Fonts/SF/SF-Pro-Display-Regular.otf#SF Pro Display</FontFamily>
             <FontFamily x:Key="SFDisplayMedium">avares://FortnitePorting/Assets/Fonts/SF/SF-Pro-Display-Medium.otf#SF Pro Display</FontFamily>
             <FontFamily x:Key="SFDisplaySemibold">avares://FortnitePorting/Assets/Fonts/SF/SF-Pro-Display-Semibold.otf#SF Pro Display</FontFamily>
             <FontFamily x:Key="SFDisplayHeavy">avares://FortnitePorting/Assets/Fonts/SF/SF-Pro-Display-Heavy.otf#SF Pro Display</FontFamily>
             <FontFamily x:Key="SFDisplayBold">avares://FortnitePorting/Assets/Fonts/SF/SF-Pro-Display-Bold.otf#SF Pro Display</FontFamily>
             <FontFamily x:Key="SFDisplayBlack">avares://FortnitePorting/Assets/Fonts/SF/SF-Pro-Display-Black.otf#SF Pro Display</FontFamily>
             
             <Brush x:Key="FPSeparatorColorLayer1">#212123</Brush>
             <Brush x:Key="FPSeparatorColorLayer2">#313133</Brush>
             
             <Brush x:Key="FPButtonBackground">#18191B</Brush>
             <Brush x:Key="FPButtonBackgroundPointerOver">#1E1F21</Brush>
             <Brush x:Key="FPButtonBackgroundPressed">#1B1C1F</Brush>
             <Brush x:Key="FPButtonBorderBrush">#27282A</Brush>
             
             <Brush x:Key="FPLayer2">#2b2c2e</Brush>
             <Brush x:Key="FPLayer2PointerOver">#3f4144</Brush>
             <Brush x:Key="FPLayer2Pressed">#353639</Brush>
             
             <Brush x:Key="FPButtonBackgroundSelected">#27282B</Brush>
             <Brush x:Key="FPButtonBackgroundSelectedPointerOver">#303236</Brush>
             
             <Brush x:Key="FPButtonBackgroundBright">#343538</Brush>
             <Brush x:Key="FPButtonBackgroundBrightPointerOver">#3e3f42</Brush>
             
         </ResourceDictionary>
         
     </Application.Resources>
             
    <Application.Styles>
        <FluentTheme />
        <fluentStyle:FluentAvaloniaTheme/>
        <material:MaterialIconStyles/>
        <StyleInclude Source="avares://FluentAvalonia.BreadcrumbBar/Styling/Styles.axaml" />
        <StyleInclude Source="avares://Avalonia.Controls.ColorPicker/Themes/Fluent/Fluent.xaml" />
        <StyleInclude Source="avares://AvaloniaEdit/Themes/Fluent/AvaloniaEdit.xaml" />
        
        <Style Selector="ui|TabViewItem /template/ Grid#LayoutRoot /template/ Border#TabContainerBorder /template/ Grid#TabContainer /template/ Button#CloseButton">
            <Setter Property="BorderBrush" Value="Transparent"/>
            <Setter Property="BorderThickness" Value="0"/>
        </Style>
        
        
        <Style Selector="ui|TabViewItem">
            <Setter Property="Background" Value="#09FFFFFF"/>
            
            <Style Selector="^ /template/ Canvas">
                <Setter Property="IsVisible" Value="False"/>
            </Style>
        </Style>
        
        <Style Selector="controls|ContentCard">
            <Setter Property="Background">
                <LinearGradientBrush StartPoint="0%, 0%" EndPoint="0%, 100%">
                    <GradientStops>
                        <GradientStop Color="#1A1B1E" Offset="0"/>
                        <GradientStop Color="#18191B" Offset="1"/>
                    </GradientStops>
                </LinearGradientBrush>
            </Setter>
            <Setter Property="BorderThickness" Value="0"/>
            <Setter Property="CornerRadius" Value="16"/>
            <Setter Property="MinHeight" Value="32"/>
            <Setter Property="ClipToBounds" Value="False"/>
            <Setter Property="BackgroundSizing" Value="CenterBorder"/>
            <Setter Property="BoxShadow">
                inset 0 1 2 -1 #50ffffff,
                inset 0 -1 2 -2 #50ffffff,
                0 4 8 #20000000,
                0 8 12 #10000000
            </Setter>
            
            <Style Selector="^:disabled">
                <Setter Property="Background">
                    <LinearGradientBrush StartPoint="0%, 0%" EndPoint="0%, 100%">
                        <GradientStops>
                            <GradientStop Color="#801A1B1E" Offset="0"/>
                            <GradientStop Color="#8018191B" Offset="1"/>
                        </GradientStops>
                    </LinearGradientBrush>
                </Setter>
            </Style>
        </Style>
        
        <Style Selector="Button[IsVisible=true]">
            <Setter Property="Background" Value="{StaticResource FPButtonBackground}"/>
            <Setter Property="BorderThickness" Value="0"/>
            <Setter Property="CornerRadius" Value="16"/>
            <Setter Property="Cursor" Value="Hand"/>
            <Setter Property="ClipToBounds" Value="False"/>
            
            <Style Selector="^ /template/ ContentPresenter#PART_ContentPresenter">
                <Setter Property="ClipToBounds" Value="False"/>
                <Setter Property="BackgroundSizing" Value="CenterBorder"/>
                <Setter Property="BoxShadow">
                    inset 0 1 2 -1 #50ffffff,
                    inset 0 -1 2 -2 #50ffffff,
                    0 4 8 #20000000,
                    0 8 12 #10000000
                </Setter>
            </Style>
        
            <Style Selector="^:pointerover /template/ ContentPresenter#PART_ContentPresenter">
                <Setter Property="Background" Value="{StaticResource FPButtonBackgroundPointerOver}"/>
            </Style>

            <Style Selector="^:pressed /template/ ContentPresenter#PART_ContentPresenter">
                <Setter Property="Background" Value="{StaticResource FPButtonBackgroundPressed}"/>
            </Style>
        
            <Style Selector="^:disabled /template/ ContentPresenter#PART_ContentPresenter">
                <Setter Property="Background" Value="{StaticResource FPButtonBackground}"/>
                <Setter Property="Opacity" Value="0.5"/>
            </Style>
        </Style>
        
        <Style Selector="breadcrumb|BreadcrumbBarItem[IsVisible=true]">
            <Setter Property="FontFamily" Value="{StaticResource SFTextRegular}"/>
            
            <Style Selector="^ Button /template/ ContentPresenter#PART_ContentPresenter">
                <Setter Property="FontFamily" Value="{StaticResource SFTextRegular}"/>
                <Setter Property="Background" Value="Transparent"/>
                <Setter Property="BoxShadow" Value="none"/>
            </Style>
        </Style>
        
        <Style Selector="Button#CloseButton">
            <Setter Property="BorderBrush" Value="Transparent"/>
            <Setter Property="BorderThickness" Value="0"/>
            <Setter Property="CornerRadius" Value="8"/>
        </Style>

        
        <Style Selector="SplitButton[IsVisible=true]">
            <Setter Property="Background" Value="{StaticResource FPButtonBackground}"/>
            <Setter Property="BorderBrush" Value="{StaticResource FPButtonBorderBrush}"/>
            <Setter Property="BorderThickness" Value="2"/>
            <Setter Property="CornerRadius" Value="16"/>
            <Setter Property="Cursor" Value="Hand"/>
            
            <Style Selector="^ /template/ Button#PART_PrimaryButton">
                <Setter Property="CornerRadius" Value="16 0 0 16"/>
            </Style>
            
            <Style Selector="^ /template/ Button#PART_SecondaryButton">
                <Setter Property="CornerRadius" Value="0 16 16 0"/>
            </Style>
            
            <Style Selector="^ /template/ Border#SeparatorBorder">
                <Setter Property="IsVisible" Value="False"/>
            </Style>
            
            <Style Selector="^:pointerover">
                <Setter Property="Background" Value="{StaticResource FPButtonBackgroundPointerOver}"/>
                <Setter Property="BorderBrush" Value="{StaticResource FPButtonBorderBrush}"/>
            </Style>

            <Style Selector="^:pressed">
                <Setter Property="Background" Value="{StaticResource FPButtonBackgroundPressed}"/>
                <Setter Property="BorderBrush" Value="{StaticResource FPButtonBorderBrush}"/>
            </Style>
        </Style>
        
        <Style Selector="ToggleButton[IsVisible=true]">
            <Setter Property="Background" Value="{StaticResource FPButtonBackground}"/>
            <Setter Property="BorderThickness" Value="0"/>
            <Setter Property="CornerRadius" Value="16"/>
            <Setter Property="Cursor" Value="Hand"/>
            <Setter Property="ClipToBounds" Value="False"/>
            
            <Style Selector="^ /template/ ContentPresenter#PART_ContentPresenter">
                <Setter Property="ClipToBounds" Value="False"/>
                <Setter Property="BackgroundSizing" Value="CenterBorder"/>
                <Setter Property="BoxShadow">
                    inset 0 1 2 -1 #50ffffff,
                    inset 0 -1 2 -2 #50ffffff,
                    0 4 8 #20000000,
                    0 8 12 #10000000
                </Setter>
            </Style>
        
            <Style Selector="^:pointerover /template/ ContentPresenter#PART_ContentPresenter">
                <Setter Property="Background" Value="{StaticResource FPButtonBackgroundPointerOver}"/>
            </Style>

            <Style Selector="^:pressed /template/ ContentPresenter#PART_ContentPresenter">
                <Setter Property="Background" Value="{StaticResource FPButtonBackgroundPressed}"/>
            </Style>
        
            <Style Selector="^:disabled /template/ ContentPresenter#PART_ContentPresenter">
                <Setter Property="Background" Value="{StaticResource FPButtonBackground}"/>
                <Setter Property="Opacity" Value="0.5"/>
            </Style>
        
            <Style Selector="^:checked /template/ ContentPresenter#PART_ContentPresenter">
                <Setter Property="Background" Value="{StaticResource FPButtonBackgroundBright}"/>
                
                <Style Selector="^:pointerover">
                    <Setter Property="Background" Value="{StaticResource FPButtonBackgroundBrightPointerOver}"/>
                </Style>
            </Style>
        </Style>
        
        <Style Selector="TextBox[IsVisible=true]">
            <Setter Property="Background" Value="{StaticResource FPButtonBackground}"/>
            <Setter Property="BorderThickness" Value="0"/>
            <Setter Property="ClipToBounds" Value="False"/>
            <Setter Property="CornerRadius" Value="16"/>
            <Setter Property="Cursor" Value="Hand"/>
            
            <Style Selector="^ /template/ DataValidationErrors">
                <Setter Property="ClipToBounds" Value="False"/>
            </Style>
            
            <Style Selector="^ /template/ ScrollViewer">
                <Setter Property="Margin" Value="{ext:Space 0.5, 0}"/>
                <Setter Property="VerticalAlignment" Value="Center"/>
            </Style>
            
            <Style Selector="^ /template/ Border#PART_BorderElement">
                <Setter Property="ClipToBounds" Value="False"/>
                <Setter Property="BackgroundSizing" Value="CenterBorder"/>
                <Setter Property="BoxShadow">
                    inset 0 1 2 -1 #50ffffff,
                    inset 0 -1 2 -2 #50ffffff,
                    0 4 8 #20000000,
                    0 8 12 #10000000
                </Setter>
            </Style>
            
            <Style Selector="^:focus /template/ Border#PART_BorderElement">
                <Setter Property="Background" Value="{StaticResource FPButtonBackground}"/>
                <Setter Property="BorderThickness" Value="0"/>
            </Style>
            
            <Style Selector="^:focus-within /template/ Border#PART_BorderElement">
                <Setter Property="Background" Value="{StaticResource FPButtonBackground}"/>
                <Setter Property="BorderThickness" Value="0"/>
            </Style>
        
            <Style Selector="^:pointerover /template/ Border#PART_BorderElement">
                <Setter Property="Background" Value="{StaticResource FPButtonBackgroundPointerOver}"/>
            </Style>

            <Style Selector="^:pressed /template/ Border#PART_BorderElement">
                <Setter Property="Background" Value="{StaticResource FPButtonBackgroundPressed}"/>
            </Style>
        
            <Style Selector="^:disabled /template/ Border#PART_BorderElement">
                <Setter Property="Background" Value="{StaticResource FPButtonBackground}"/>
                <Setter Property="Opacity" Value="0.5"/>
            </Style>
        </Style>
        
        <Style Selector="ListBoxItem[IsVisible=True]">
            <Setter Property="FontFamily" Value="{StaticResource SFTextRegular}"/>
            <Setter Property="Background" Value="{StaticResource FPButtonBackground}"/>
            <Setter Property="BorderThickness" Value="0"/>
            <Setter Property="ClipToBounds" Value="False"/>
            <Setter Property="CornerRadius" Value="16"/>
            <Setter Property="Cursor" Value="Hand"/>
            
            <Style Selector="^:selected /template/ ContentPresenter#PART_ContentPresenter">
                <Setter Property="Background" Value="{StaticResource FPButtonBackgroundSelected}"/>
                <Setter Property="ClipToBounds" Value="False"/>
                <Setter Property="BackgroundSizing" Value="CenterBorder"/>
                <Setter Property="BoxShadow">
                    inset 0 1 2 -1 #50ffffff,
                    inset 0 -1 2 -2 #50ffffff,
                    0 4 8 #20000000,
                    0 8 12 #10000000
                </Setter>
            </Style>
            
            <Style Selector="^:not(:selected)">
                <Setter Property="Background" Value="Transparent"/>
            </Style>
            
            
            <Style Selector="^ /template/ Rectangle#SelectionIndicator">
                <Setter Property="Opacity" Value="0" />
            </Style>
        </Style>
        
        <Style Selector="FlyoutPresenter[IsVisible=true]">
            <Setter Property="Padding" Value="{ext:Space 1}"/>
            
            <Style Selector="^ ListBoxItem">
                <Setter Property="CornerRadius" Value="12"/>
            </Style>
            
            <Style Selector="^ /template/ Border">
                <Setter Property="Background">
                    <LinearGradientBrush StartPoint="0%, 0%" EndPoint="0%, 100%">
                        <GradientStops>
                            <GradientStop Color="#1A1B1E" Offset="0"/>
                            <GradientStop Color="#18191B" Offset="1"/>
                        </GradientStops>
                    </LinearGradientBrush>
                </Setter>
                
                <Setter Property="MinWidth" Value="{Binding $parent[DropDownButton].Bounds.Width}"/>
                <Setter Property="BorderThickness" Value="0"/>
                <Setter Property="CornerRadius" Value="16"/>
                <Setter Property="Margin" Value="{ext:Space 1}"/>
                <Setter Property="ClipToBounds" Value="False"/>
                <Setter Property="BackgroundSizing" Value="CenterBorder"/>
                <Setter Property="BoxShadow">
                    inset 0 1 2 -1 #50ffffff,
                    inset 0 -1 2 -2 #50ffffff,
                    0 4 8 #20000000,
                    0 8 12 #10000000
                </Setter>
            </Style>
        </Style>
        
        <Style Selector="PopupRoot">
            <Setter Property="ClipToBounds" Value="False"/>
        </Style>
        
        <Style Selector="ToolTip[IsVisible=true]">
            <Setter Property="ClipToBounds" Value="False"/>
            <Setter Property="FontFamily" Value="{StaticResource SFTextRegular}"/>
            <Setter Property="Padding" Value="{ext:Space 1}"/>
            
            <Style Selector="^ /template/ Border#PART_LayoutRoot">
                <Setter Property="Background">
                    <LinearGradientBrush StartPoint="0%, 0%" EndPoint="0%, 100%">
                        <GradientStops>
                            <GradientStop Color="#1A1B1E" Offset="0"/>
                            <GradientStop Color="#18191B" Offset="1"/>
                        </GradientStops>
                    </LinearGradientBrush>
                </Setter>
                
                <Setter Property="MinWidth" Value="{Binding $parent[DropDownButton].Bounds.Width}"/>
                <Setter Property="BorderThickness" Value="0"/>
                <Setter Property="CornerRadius" Value="16"/>
                <Setter Property="Margin" Value="{ext:Space 1}"/>
                <Setter Property="ClipToBounds" Value="False"/>
                <Setter Property="BackgroundSizing" Value="CenterBorder"/>
                <Setter Property="BoxShadow">
                    inset 0 1 2 -1 #50ffffff,
                    inset 0 -1 2 -2 #50ffffff,
                    0 4 8 #20000000,
                    0 8 12 #10000000
                </Setter>
            </Style>
        </Style>
        
        
        <Style Selector="ComboBox[IsVisible=true]">
            <Setter Property="Background" Value="{StaticResource FPButtonBackground}"/>
            <Setter Property="BorderThickness" Value="0"/>
            <Setter Property="CornerRadius" Value="16"/>
            <Setter Property="ClipToBounds" Value="False"/>
            <Setter Property="Cursor" Value="Hand"/>
            <Setter Property="VerticalContentAlignment" Value="Center"/>
            
            <Style Selector="^ /template/ ContentPresenter#ContentPresenter">
                <Setter Property="Margin" Value="12 0 0 0"/>
            </Style>
            
            <Style Selector="^ /template/ DataValidationErrors">
                <Setter Property="ClipToBounds" Value="False"/>
            </Style>
            
            <Style Selector="^ /template/ Border#Background">
                <Setter Property="ClipToBounds" Value="False"/>
                <Setter Property="BackgroundSizing" Value="CenterBorder"/>
                <Setter Property="BoxShadow">
                    inset 0 1 2 -1 #50ffffff,
                    inset 0 -1 2 -2 #50ffffff,
                    0 4 8 #20000000,
                    0 8 12 #10000000
                </Setter>
            </Style>

            <Style Selector="^/template/ Border#PopupBorder">
                <Setter Property="Background">
                    <LinearGradientBrush StartPoint="0%, 0%" EndPoint="0%, 100%">
                        <GradientStops>
                            <GradientStop Color="#1A1B1E" Offset="0"/>
                            <GradientStop Color="#18191B" Offset="1"/>
                        </GradientStops>
                    </LinearGradientBrush>
                </Setter>
                
                <Setter Property="MinWidth" Value="{Binding $parent[ComboBox].Bounds.Width}"/>
                <Setter Property="BorderThickness" Value="0"/>
                <Setter Property="CornerRadius" Value="16"/>
                <Setter Property="Padding" Value="4"/>
                <Setter Property="Margin" Value="{ext:Space 1}"/>
                <Setter Property="ClipToBounds" Value="False"/>
                <Setter Property="BackgroundSizing" Value="CenterBorder"/>
                <Setter Property="BoxShadow">
                    inset 0 1 2 -1 #50ffffff,
                    inset 0 -1 2 -2 #50ffffff,
                    0 0 8 #20000000,
                    0 0 12 #10000000
                </Setter>
            </Style>

            <Style Selector="^ Popup">
                <Setter Property="VerticalOffset" Value="4"/>
                <Setter Property="ClipToBounds" Value="False"/>

            </Style>
        </Style>
        
        <Style Selector="TreeViewItem[IsVisible=True]">
            <Setter Property="FontFamily" Value="{StaticResource SFTextRegular}"/>
            <Setter Property="CornerRadius" Value="12"/>
            <Setter Property="ClipToBounds" Value="False"/>
            
            <Style Selector="^:selected /template/ Border#PART_LayoutRoot">
                <Setter Property="Background" Value="{StaticResource FPButtonBackgroundSelected}"/>
                <Setter Property="ClipToBounds" Value="False"/>
                <Setter Property="BackgroundSizing" Value="CenterBorder"/>
                <Setter Property="BoxShadow">
                    inset 0 1 2 -1 #50ffffff,
                    inset 0 -1 2 -2 #50ffffff,
                    0 4 8 #20000000,
                    0 8 12 #10000000
                </Setter>
                
                <Style Selector="^:pointerover">
                    <Setter Property="Background" Value="{StaticResource FPButtonBackgroundSelectedPointerOver}"/>
                </Style>
            </Style>
            
            <Style Selector="^ /template/ Rectangle#SelectionIndicator">
                <Setter Property="Opacity" Value="0" />
            </Style>
        </Style>

        
        <Style Selector="ComboBoxItem[IsVisible=True]">
            <Setter Property="CornerRadius" Value="12"/>
            <Setter Property="ClipToBounds" Value="False"/>
            
            <Style Selector="^:selected /template/ Border#LayoutRoot">
                <Setter Property="Background" Value="{StaticResource FPButtonBackgroundSelected}"/>
                <Setter Property="ClipToBounds" Value="False"/>
                <Setter Property="BackgroundSizing" Value="CenterBorder"/>
                <Setter Property="BoxShadow">
                    inset 0 1 2 -1 #50ffffff,
                    inset 0 -1 2 -2 #50ffffff,
                    0 4 8 #20000000,
                    0 8 12 #10000000
                </Setter>
                
                <Style Selector="^:pointerover">
                    <Setter Property="Background" Value="{StaticResource FPButtonBackgroundSelectedPointerOver}"/>
                </Style>
            </Style>
            
            <Style Selector="^ /template/ Rectangle#Pill">
                <Setter Property="Opacity" Value="0" />
            </Style>
        </Style>
        
        <Style Selector="CheckBox[IsVisible=true]">
            <Setter Property="FontFamily" Value="{StaticResource SFTextRegular}"/>
            <Setter Property="VerticalContentAlignment" Value="Center"/>
            
            <Style Selector="^ /template/ ContentPresenter#ContentPresenter">
                <Setter Property="Margin" Value="{ext:Space 1, 0}"/>
            </Style>
            
            <Style Selector="^ /template/ ui|FontIcon#CheckGlyph">
                <Setter Property="IsHitTestVisible" Value="False"/>
            </Style>
            
            <Style Selector="^ /template/ Border#NormalRectangle">
                <Setter Property="Background" Value="{StaticResource FPButtonBackgroundSelected}"/>
                <Setter Property="BorderThickness" Value="0"/>
                <Setter Property="CornerRadius" Value="16"/>
                <Setter Property="ClipToBounds" Value="False"/>
                <Setter Property="BackgroundSizing" Value="CenterBorder"/>
                <Setter Property="BoxShadow">
                    inset 0 1 2 -1 #50ffffff,
                    inset 0 -1 2 -2 #50ffffff,
                    0 4 8 #20000000,
                    0 8 12 #10000000
                </Setter>
                
                <Style Selector="^:pointerover">
                    <Setter Property="Background" Value="{StaticResource FPButtonBackgroundSelectedPointerOver}"/>
                </Style>
            </Style>
            
            <Style Selector="^:checked /template/ Border#NormalRectangle">
                <Setter Property="Background" Value="{StaticResource FPButtonBackgroundBright}"/>
                
                <Style Selector="^:pointerover">
                    <Setter Property="Background" Value="{StaticResource FPButtonBackgroundBrightPointerOver}"/>
                </Style>
            </Style>
            
            <Style Selector="^ TextBlock">
                <Setter Property="Foreground" Value="#aeafb3"/>
            </Style>
        </Style>
        
        <Style Selector="ScrollBar">
            <Setter Property="Opacity" Value="0"/>
            
            <Style Selector="^:pointerover">
                <Setter Property="Opacity" Value="1"/>
                <Setter Property="Margin" Value="{ext:Space 25, 0, 0, 0}"/>
            </Style>
        </Style>
        
        <Style Selector="DropDownButton[IsVisible=True]">
            <Setter Property="Background" Value="{StaticResource FPButtonBackground}"/>
            <Setter Property="BorderThickness" Value="0"/>
            <Setter Property="CornerRadius" Value="16"/>
            <Setter Property="ClipToBounds" Value="False"/>
            <Setter Property="Cursor" Value="Hand"/>
            
            <Style Selector="^ /template/ Border#RootBorder">
                <Setter Property="ClipToBounds" Value="False"/>
                <Setter Property="BackgroundSizing" Value="CenterBorder"/>
                <Setter Property="BoxShadow">
                    inset 0 1 2 -1 #50ffffff,
                    inset 0 -1 2 -2 #50ffffff,
                    0 4 8 #20000000,
                    0 8 12 #10000000
                </Setter>
            </Style>
        </Style>
        
        <Style Selector="Border">
            <Setter Property="CornerRadius" Value="4"/>
        </Style>
        
        <Style Selector="Border.MinimizeMaximize">
            <Setter Property="Background" Value="Transparent"/>
            
            <Style Selector="^:pointerover">
                <Setter Property="Background" Value="#0AFFFFFF"/>
            </Style>
        </Style>
        
        <Style Selector="ProgressBar[IsVisible=true]">
            <Setter Property="Height" Value="16"/>
            <Setter Property="ClipToBounds" Value="False"/>
            
            <Style Selector="^ Border#PART_Indicator">
                <Setter Property="Background" Value="#953bf8"/>
                <Setter Property="Height" Value="NaN"/>
                <Setter Property="CornerRadius" Value="16"/>
                
                <Setter Property="BoxShadow">
